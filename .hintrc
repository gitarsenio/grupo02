{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "frame-title": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "button-type": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "accent-color"
        ]
      }
    ],
    "no-protocol-relative-urls": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 123",
    "not firefox <= 123",
    "not ios_saf <= 17.3",
    "not safari <= 17.3"
  ]
}